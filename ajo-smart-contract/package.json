{
  "name": "ajo-save",
  "version": "3.0.0",
  "description": "Decentralized Rotating Savings and Credit Association (ROSCA) protocol on Hedera with V3 60% collateral model, HCS-enabled governance, and factory scaling",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "deploy:hedera-testnet": "hardhat run scripts/deploy-hardhat.cjs --network hedera_testnet",
    "deploy:hedera-mainnet": "hardhat run scripts/deploy-hardhat.cjs --network hedera_mainnet",
    "deploy:hedera-local": "hardhat run scripts/deploy-hardhat.cjs --network hedera_local",
    "deploy:hcs-governance": "hardhat run scripts/deploy-hcs-governance.js --network hedera_testnet",
    "deploy:4-phase-factory": "hardhat run scripts/deploy-4-phase-factory.js --network hedera_testnet",
    "deploy:task-testnet": "hardhat deploy-ajo-system --network hedera_testnet",
    "deploy:task-mainnet": "hardhat deploy-ajo-system --network hedera_mainnet",
    "demo:complete": "hardhat run scripts/hackathon-demo-complete.cjs --network hedera_testnet",
    "demo:core": "hardhat run scripts/test-core-functions.cjs --network hedera_testnet",
    "check-balance": "hardhat show-balance --network hedera_testnet",
    "aggregator:register": "node scripts/HCSVoteAggregator.js register",
    "aggregator:monitor": "node scripts/HCSVoteAggregator.js monitor",
    "aggregator:stats": "node scripts/HCSVoteAggregator.js stats",
    "vote:list": "node scripts/HCSVoteClient.js list",
    "vote:power": "node scripts/HCSVoteClient.js power",
    "vote:cast": "node scripts/HCSVoteClient.js vote",
    "vote:info": "node scripts/HCSVoteClient.js info",
    "lint": "solhint 'contracts/**/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write 'contracts/**/*.sol' 'scripts/**/*.js' 'test/**/*.js'",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "flatten": "hardhat flatten"
  },
  "keywords": [
    "hedera",
    "rosca",
    "ajo",
    "esusu",
    "defi",
    "savings-circle",
    "rotating-savings",
    "collateral-system",
    "hcs",
    "hedera-consensus-service",
    "governance",
    "factory-pattern",
    "minimal-proxy",
    "web3",
    "blockchain",
    "smart-contracts",
    "traditional-finance",
    "financial-inclusion"
  ],
  "author": "Ajo.save Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@openzeppelin/hardhat-upgrades": "^1.28.0",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.1",
    "chai": "^4.3.7",
    "ethers": "^5.8.0",
    "hardhat": "^2.17.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.2",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.6.2",
    "solidity-coverage": "^0.8.4",
    "ts-node": "^10.9.1",
    "typechain": "^8.3.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "@hashgraph/sdk": "^2.35.0",
    "ethers": "^5.7.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ola-893/ajo-contract.git"
  },
  "bugs": {
    "url": "https://github.com/ola-893/ajo-contract/issues"
  },
  "homepage": "https://github.com/ola-893/ajo-contract#readme"
}